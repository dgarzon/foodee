!!!
%html
  %head
    %title Foodee - #{yield(:title)}
    = google_webfonts_link_tag :dosis => [500, 600]
    = google_webfonts_link_tag :open_sans => [400]
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
  %body
    %header

    %nav
      = render 'layouts/nav'
    %div.container#main
      = render 'layouts/notice'
      = yield
    %footer
